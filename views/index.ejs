    <h4 style="margin-top: 5rem;">Daftar mahasiswa</h4>
    <% if (mahasiswa.length < 1) { %>
        <p>Data mahasiswa masih kosong</p>
    <% }else { %>
        <% mahasiswa.forEach(mhs => { %>
            <ul>
                <li>Nama: <%=mhs.nama  %> </li>
                <li>Nama: <%=mhs.email  %> </li>
            </ul>
        <% }) %>        
    <% } %>
